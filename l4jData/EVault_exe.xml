<launch4jConfig>
  <headerType>gui</headerType>
  <jar>../EVault.jar</jar>
  <outfile>../EVault.exe</outfile>
  <errTitle>EVault</errTitle>
  <chdir>.</chdir>
  <icon>EVault.ico</icon>
  <restartOnCrash>true</restartOnCrash>
  <manifest>EVault.manifest</manifest>
  <jre>
    <minVersion>1.6.0</minVersion>
  </jre>
  <splash>
    <file>EVault.png</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
</launch4jConfig>
